{"ast":null,"code":"import LoaderMarselotech from '@/components/LoaderMarselotech.vue';\nimport { autenticatUser } from '@/firebase';\nimport { reactive } from 'vue';\nimport { useRouter } from 'vue-router';\nexport default {\n  name: \"LogiDashboard\",\n\n  setup() {\n    const router = useRouter();\n    const form = reactive({\n      email: '',\n      robotid: ''\n    });\n\n    const onSubmit = async () => {\n      const responseAuth = await autenticatUser({ ...form\n      });\n\n      switch (responseAuth.status) {\n        case 0:\n          router.push({\n            name: \"dashboard\",\n            params: {\n              robotid: responseAuth.data.robotid\n            }\n          });\n          break;\n\n        case 1:\n          form.email = '';\n          form.robotid = '';\n          break;\n\n        case 2:\n          form.email = '';\n          form.robotid = '';\n          break;\n\n        default:\n          form.email = '';\n          form.robotid = '';\n      }\n    };\n\n    return {\n      form,\n      onSubmit\n    };\n  },\n\n  components: {\n    LoaderMarselotech\n  }\n};","map":{"version":3,"mappings":"AA+CA,OAAOA,iBAAP,MAA8B,oCAA9B;AACA,SAAQC,cAAR,QAA6B,YAA7B;AACA,SAASC,QAAT,QAAyB,KAAzB;AACA,SAASC,SAAT,QAA0B,YAA1B;AAGA,eAAc;EACVC,IAAI,EAAE,eADI;;EAEVC,KAAK,GAAG;IAEJ,MAAMC,MAAK,GAAIH,SAAS,EAAxB;IAGA,MAAMI,IAAG,GAAIL,QAAQ,CAAC;MAACM,KAAK,EAAC,EAAP;MAAUC,OAAO,EAAC;IAAlB,CAAD,CAArB;;IAEA,MAAMC,QAAO,GAAK,YAAY;MAE3B,MAAMC,YAAW,GAAK,MAAOV,cAAc,CAAC,EAAC,GAAGM;MAAJ,CAAD,CAA3C;;MACA,QAAOI,YAAY,CAACC,MAApB;QACI,KAAK,CAAL;UACIN,MAAM,CAACO,IAAP,CAAY;YACPT,IAAI,EAAE,WADC;YAEPU,MAAM,EAAC;cACHL,OAAO,EAAEE,YAAY,CAACI,IAAb,CAAkBN;YADxB;UAFA,CAAZ;UAMH;;QAEA,KAAK,CAAL;UACIF,IAAI,CAACC,KAAL,GAAa,EAAb;UACAD,IAAI,CAACE,OAAL,GAAe,EAAf;UACJ;;QAEA,KAAK,CAAL;UACIF,IAAI,CAACC,KAAL,GAAa,EAAb;UACAD,IAAI,CAACE,OAAL,GAAe,EAAf;UACJ;;QAEA;UACIF,IAAI,CAACC,KAAL,GAAa,EAAb;UACAD,IAAI,CAACE,OAAL,GAAe,EAAf;MAtBT;IAyBH,CA5BA;;IA8BA,OAAO;MAAEF,IAAF;MAAQG;IAAR,CAAP;EACH,CAxCS;;EAyCVM,UAAU,EAAE;IAAEhB;EAAF;AAzCF,CAAd","names":["LoaderMarselotech","autenticatUser","reactive","useRouter","name","setup","router","form","email","robotid","onSubmit","responseAuth","status","push","params","data","components"],"sourceRoot":"","sources":["/opt/lampp/htdocs/marselotech-web/marselotech-web/src/views/LoginDashboard.vue"],"sourcesContent":["<template>\n    <!--<LoaderMarselotech/>-->\n    <div class='page-wrap' id='fullpage'>\n    <!-- Particles Background -->\n        <canvas class='bg-particles' id='bg-particles'></canvas>\n\n        <section class=\"welcome section-content section background-parallax bg-image-1\">\n            <div class='overlay'></div>\n            <div class=\"container\" style=\"padding-right: 0;padding-left: 0;margin-right: 0;margin-left: 0;\">\n                <div class=\"row g-0\">\n                    <div class=\"col-lg-6 bg-dark d-flex flex-column align-items-end min-vh-100\">\n                        <!--<div class=\"px-lg-5 pt-lg-4 pb-lg-3 p-4 mb-auto w-100\">\n                            <img src=\"Logo.svg\" class=\"img-fluid\" />\n                        </div>-->\n                        <div class=\"align-self-center my-auto w-100 px-lg-5 py-lg-4 p-4\">\n                            <h1 class=\"font-weight-bold mb-4 text-light\">Bienvenido a Marselotech</h1>\n                            <form @submit.prevent=\"onSubmit\" class=\"mb-5\">\n                                <div class=\"mb-4\">\n                                    <label for=\"exampleInputEmail1\" class=\"form-label font-weight-bold text-light\">Email</label>\n                                    <input v-model=\"form.email\" type=\"email\" class=\"form-control bg-dark-x border-0\" id=\"exampleInputEmail1\" placeholder=\"Ingresa tu email\" aria-describedby=\"emailHelp\">\n                                </div>\n                                <div class=\"mb-4\">\n                                    <label for=\"exampleInputPassword1\" class=\"form-label font-weight-bold text-light\">Código del robot</label>\n                                    <input v-model=\"form.robotid\" type=\"password\" class=\"form-control bg-dark-x border-0 mb-2\" placeholder=\"Ingresa tu código del robot\" id=\"exampleInputPassword1\">\n                                    <a href=\"\" id=\"emailHelp\" class=\"form-text text-muted text-decoration-none\">¿Has olvidado tu código?</a>\n                                </div>\n\n                                \n                                <button type=\"submit\" class=\"btn btn-primary w-100\">Entrar</button>\n                                \n                                \n\n                            </form>\n                        </div>\n                    </div>\n                    <div class=\"col-lg-6 d-none d-lg-block\">\n\n                    </div>\n\n                </div>\n            </div>\n\n        </section>\n    </div>\n</template>\n\n<script>\nimport LoaderMarselotech from '@/components/LoaderMarselotech.vue';\nimport {autenticatUser} from '@/firebase'\nimport { reactive } from 'vue';\nimport { useRouter } from 'vue-router'\n\n\nexport default{\n    name: \"LogiDashboard\",\n    setup() {\n\n        const router = useRouter();\n    \n\n        const form = reactive({email:'',robotid:''});\n\n        const onSubmit =  async () => {\n\n           const responseAuth =  await  autenticatUser({...form});\n           switch(responseAuth.status){\n               case 0:\n                   router.push({\n                        name: \"dashboard\",\n                        params:{\n                            robotid: responseAuth.data.robotid,\n                        }\n                    });\n                break;\n                \n                case 1:\n                    form.email = '';\n                    form.robotid = '';\n                break;\n\n                case 2:\n                    form.email = '';\n                    form.robotid = '';\n                break;\n\n                default:\n                    form.email = '';\n                    form.robotid = '';\n           }\n\n        }\n\n        return { form, onSubmit};\n    },\n    components: { LoaderMarselotech }\n}\n</script>\n\n\n<style>\n\n</style>"]},"metadata":{},"sourceType":"module"}