order-0 mb-2\" placeholder=\"Ingresa tu código del robot\" id=\"exampleInputPassword1\">\n                                    <a href=\"\" id=\"emailHelp\" class=\"form-text text-muted text-decoration-none\">¿Has olvidado tu código?</a>\n                                </div>\n\n                                \n                                <button type=\"submit\" class=\"btn btn-primary w-100\">Entrar</button>\n                                \n                                \n\n                            </form>\n                        </div>\n                    </div>\n                    <div class=\"col-lg-6 d-none d-lg-block\">\n\n                    </div>\n\n                </div>\n            </div>\n\n        </section>\n    </div>\n</template>\n\n<script>\nimport LoaderMarselotech from '@/components/LoaderMarselotech.vue';\nimport {autenticatUser} from '@/firebase'\nimport { reactive } from 'vue';\nimport { useRouter } from 'vue-router'\n\n\nexport default{\n    name: \"LogiDashboard\",\n    setup() {\n\n        const router = useRouter();\n    \n        const autenticate = () => {\n            console.log(\"Dengue\");\n        };\n\n        const form = reactive({email:'',robotid:''});\n\n        /*const onSubmit = async () => {\n            await createUser({...form})\n            form.email = ''\n            form.password = ''\n        }*/\n\n        const onSubmit =  async () => {\n\n           const responseAuth =  await  autenticatUser({...form})\n           console.log(responseAuth);\n            form.email = '';\n            form.robotid = '';\n\n            router.push({\n                    name: \"dashboard\",\n                    params:{\n                      robotid: \"xpyBkLbT3T5A2BwvuPSW\",\n                    }\n            });\n\n        }\n\n        return { autenticate, form, onSubmit};\n    },\n    components: { LoaderMarselotech }\n}\n</script>\n\n\n<style>\n\n</style>"]},"metadata":{},"sourceType":"module"}