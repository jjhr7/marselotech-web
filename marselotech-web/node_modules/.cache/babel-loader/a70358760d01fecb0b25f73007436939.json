{"ast":null,"code":"import { reactive, onMounted } from 'vue';\nimport { useRouter } from 'vue-router';\nimport { getLastImage } from '@/firebase';\nimport { connect, disconnect, call_delante_service, detectar_caras, detectar_personas, detectar_enemigos, detectar_armas, enviar_coordenada } from '@/robot';\nexport default {\n  name: 'DashboardMarselotech',\n\n  setup() {\n    onMounted(() => {\n      setTimeout(() => {//w\n\n        /* document.addEventListener(\"keydown\", (w) => {\n             call_movement_service_robot('delante');\n         })\n          document.addEventListener('input', (w) => {\n             logMessage(`Key \"${w.data}\" input  [event: input]`);\n             call_movement_service_robot('delante');\n         });\n           //a\n         document.addEventListener(\"keydown\", (a) => {\n             call_movement_service_robot('izquierda');\n         })\n          document.addEventListener('input', (a) => {\n             logMessage(`Key \"${a.data}\" input  [event: input]`);\n             call_movement_service_robot('izquierda');\n         });\n           //s\n         document.addEventListener(\"keydown\", (s) => {\n             call_movement_service_robot('atras')\n         })\n          document.addEventListener('input', (s) => {\n             logMessage(`Key \"${s.data}\" input  [event: input]`);\n             call_movement_service_robot('atras')\n         });\n           //d\n         document.addEventListener(\"keydown\", (d) => {\n             call_movement_service_robot('derecha')\n         })\n          document.addEventListener('input', (d) => {\n             logMessage(`Key \"${d.data}\" input  [event: input]`);\n             call_movement_service_robot('derecha')\n         });*/\n      }, 1500);\n    });\n    const router = useRouter();\n\n    const changeTabActive = num => {\n      document.getElementById(\"ex-with-icons-tab-1\").classList.remove('active');\n\n      if (num === 1) {\n        //ex-with-icons-tab-1\n        document.getElementById(\"ex-with-icons-tab-1\").classList.add('active');\n        document.getElementById(\"ex-with-icons-tab-2\").classList.remove('active');\n        document.getElementById(\"ex-with-icons-tab-3\").classList.remove('active');\n      } else if (num === 2) {\n        // ex-with-icons-tab-2\n        //ex-with-icons-tab-1\n        document.getElementById(\"ex-with-icons-tab-2\").classList.add('active');\n        document.getElementById(\"ex-with-icons-tab-1\").classList.remove('active');\n        document.getElementById(\"ex-with-icons-tab-3\").classList.remove('active');\n      } else if (num === 3) {\n        // ex-with-icons-tab-3\n        document.getElementById(\"ex-with-icons-tab-3\").classList.add('active');\n        document.getElementById(\"ex-with-icons-tab-2\").classList.remove('active');\n        document.getElementById(\"ex-with-icons-tab-1\").classList.remove('active');\n      }\n    };\n\n    const getLastImageFromFirebase = async imgType => {\n      /*caras, persona, color, arma*/\n      const imgUrl = await getLastImage(imgType);\n      return imgUrl;\n    };\n\n    const call_getLastImg = async (imgTypetxt, idComponent) => {\n      var curretnUrl = new URL(window.location.href);\n      let ulrtxt = curretnUrl.toString();\n      let urlSplit = ulrtxt.split(\"/\");\n      let urlLength = urlSplit.length;\n      let robotid = urlSplit[urlLength - 1];\n      let imgType = {\n        idRobot: robotid,\n        type: imgTypetxt\n      };\n      let imageWeb = document.getElementById(idComponent);\n      const urlimgage = await getLastImageFromFirebase(imgType);\n      console.log(urlimgage);\n      imageWeb.src = urlimgage;\n    };\n\n    const connectRobot = connect;\n    const disconnectRobot = disconnect;\n\n    const call_detectar_caras_service = () => {\n      call_getLastImg(\"caras\", \"tabApoyo\");\n      detectar_caras();\n    };\n\n    const call_detectar_personas_service = () => {\n      call_getLastImg(\"persona\", \"tabApoyo\");\n      detectar_personas();\n    };\n\n    const call_detectar_enemigos_service = () => {\n      call_getLastImg(\"color\", \"tabApoyo\");\n      detectar_enemigos();\n    };\n\n    const call_detectar_armas_robot = () => {\n      call_getLastImg(\"arma\", \"tabApoyo\");\n      detectar_armas();\n    };\n\n    const call_movement_service_robot = async valor => {\n      await call_delante_service(valor);\n    };\n\n    const call_enviar_cords = async () => {\n      let x = document.getElementById(\"x_input\").value;\n      let y = document.getElementById(\"y_input\").value;\n      await enviar_coordenada(x, y);\n    };\n\n    return {\n      call_getLastImg,\n      changeTabActive,\n      connectRobot,\n      disconnectRobot,\n      call_movement_service_robot,\n      call_detectar_caras_service,\n      call_detectar_personas_service,\n      call_detectar_enemigos_service,\n      call_detectar_armas_robot,\n      call_enviar_cords\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAoNA,SAASA,QAAT,EAAmBC,SAAnB,QAAoC,KAApC;AACA,SAASC,SAAT,QAA0B,YAA1B;AACA,SAAQC,YAAR,QAA2B,YAA3B;AACA,SAASC,OAAT,EAAkBC,UAAlB,EAA8BC,oBAA9B,EAAoDC,cAApD,EAAoEC,iBAApE,EAAuFC,iBAAvF,EAA0GC,cAA1G,EAA0HC,iBAA1H,QAAkJ,SAAlJ;AAGA,eAAc;EACVC,IAAI,EAAE,sBADI;;EAEVC,KAAK,GAAE;IAEHZ,SAAS,CAAC,MAAI;MACVa,UAAU,CAAC,MAAM,CAEb;;QACD;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;MA2CF,CA9CS,EA8CR,IA9CQ,CAAV;IA+CH,CAhDQ,CAAT;IAkDA,MAAMC,MAAK,GAAIb,SAAS,EAAxB;;IAEA,MAAMc,eAAc,GAAKC,GAAD,IAAQ;MAC5BC,QAAQ,CAACC,cAAT,CAAwB,qBAAxB,EAA+CC,SAA/C,CAAyDC,MAAzD,CAAgE,QAAhE;;MACA,IAAGJ,GAAE,KAAM,CAAX,EAAa;QACT;QACAC,QAAQ,CAACC,cAAT,CAAwB,qBAAxB,EAA+CC,SAA/C,CAAyDE,GAAzD,CAA6D,QAA7D;QACAJ,QAAQ,CAACC,cAAT,CAAwB,qBAAxB,EAA+CC,SAA/C,CAAyDC,MAAzD,CAAgE,QAAhE;QACAH,QAAQ,CAACC,cAAT,CAAwB,qBAAxB,EAA+CC,SAA/C,CAAyDC,MAAzD,CAAgE,QAAhE;MACH,CALD,MAKM,IAAGJ,GAAE,KAAM,CAAX,EAAa;QACf;QACA;QACAC,QAAQ,CAACC,cAAT,CAAwB,qBAAxB,EAA+CC,SAA/C,CAAyDE,GAAzD,CAA6D,QAA7D;QACAJ,QAAQ,CAACC,cAAT,CAAwB,qBAAxB,EAA+CC,SAA/C,CAAyDC,MAAzD,CAAgE,QAAhE;QACAH,QAAQ,CAACC,cAAT,CAAwB,qBAAxB,EAA+CC,SAA/C,CAAyDC,MAAzD,CAAgE,QAAhE;MACH,CANK,MAMA,IAAGJ,GAAE,KAAM,CAAX,EAAa;QACf;QACAC,QAAQ,CAACC,cAAT,CAAwB,qBAAxB,EAA+CC,SAA/C,CAAyDE,GAAzD,CAA6D,QAA7D;QACAJ,QAAQ,CAACC,cAAT,CAAwB,qBAAxB,EAA+CC,SAA/C,CAAyDC,MAAzD,CAAgE,QAAhE;QACAH,QAAQ,CAACC,cAAT,CAAwB,qBAAxB,EAA+CC,SAA/C,CAAyDC,MAAzD,CAAgE,QAAhE;MACJ;IACJ,CAnBA;;IAqBA,MAAME,wBAAuB,GAAI,MAAOC,OAAP,IAAmB;MAChD;MACD,MAAMC,MAAK,GAAI,MAAMtB,YAAY,CAACqB,OAAD,CAAjC;MACA,OAAOC,MAAP;IACH,CAJA;;IAMA,MAAMC,eAAc,GAAI,OAAOC,UAAP,EAAkBC,WAAlB,KAAkC;MAEtD,IAAIC,UAAS,GAAI,IAAIC,GAAJ,CAAQC,MAAM,CAACC,QAAP,CAAgBC,IAAxB,CAAjB;MACA,IAAIC,MAAK,GAAIL,UAAU,CAACM,QAAX,EAAb;MACA,IAAIC,QAAO,GAAIF,MAAM,CAACG,KAAP,CAAa,GAAb,CAAf;MACA,IAAIC,SAAQ,GAAIF,QAAQ,CAACG,MAAzB;MACA,IAAIC,OAAM,GAAIJ,QAAQ,CAACE,SAAS,GAAC,CAAX,CAAtB;MACA,IAAId,OAAM,GAAI;QAACiB,OAAO,EAAED,OAAV;QAAmBE,IAAI,EAAEf;MAAzB,CAAd;MACA,IAAIgB,QAAO,GAAIzB,QAAQ,CAACC,cAAT,CAAwBS,WAAxB,CAAf;MACA,MAAMgB,SAAQ,GAAI,MAAMrB,wBAAwB,CAACC,OAAD,CAAhD;MACAqB,OAAO,CAACC,GAAR,CAAYF,SAAZ;MACAD,QAAQ,CAACI,GAAT,GAAeH,SAAf;IAEJ,CAbA;;IAgBA,MAAOI,YAAW,GAAI5C,OAAtB;IAEA,MAAM6C,eAAc,GAAI5C,UAAxB;;IAEA,MAAM6C,2BAA0B,GAAK,MAAK;MACtCxB,eAAe,CAAC,OAAD,EAAS,UAAT,CAAf;MACAnB,cAAc;IACjB,CAHD;;IAKA,MAAM4C,8BAA6B,GAAI,MAAM;MACzCzB,eAAe,CAAC,SAAD,EAAW,UAAX,CAAf;MACAlB,iBAAiB;IACpB,CAHD;;IAKA,MAAM4C,8BAA6B,GAAI,MAAK;MACxC1B,eAAe,CAAC,OAAD,EAAS,UAAT,CAAf;MACAjB,iBAAiB;IACpB,CAHD;;IAKA,MAAM4C,yBAAwB,GAAI,MAAM;MACpC3B,eAAe,CAAC,MAAD,EAAQ,UAAR,CAAf;MACAhB,cAAc;IACjB,CAHD;;IAKA,MAAM4C,2BAA0B,GAAI,MAAOC,KAAP,IAAiB;MAClD,MAAMjD,oBAAoB,CAACiD,KAAD,CAA1B;IACF,CAFD;;IAIA,MAAMC,iBAAgB,GAAI,YAAY;MAClC,IAAIC,IAAIvC,QAAQ,CAACC,cAAT,CAAwB,SAAxB,EAAmCuC,KAA3C;MACA,IAAIC,IAAIzC,QAAQ,CAACC,cAAT,CAAwB,SAAxB,EAAmCuC,KAA3C;MACA,MAAM/C,iBAAiB,CAAC8C,CAAD,EAAGE,CAAH,CAAvB;IAEH,CALD;;IASA,OAAO;MAAEjC,eAAF;MAAmBV,eAAnB;MAAoCgC,YAApC;MAAkDC,eAAlD;MAAmEK,2BAAnE;MAAgGJ,2BAAhG;MAA6HC,8BAA7H;MAA6JC,8BAA7J;MAA6LC,yBAA7L;MAAwNG;IAAxN,CAAP;EACJ;;AAzIU,CAAd","names":["reactive","onMounted","useRouter","getLastImage","connect","disconnect","call_delante_service","detectar_caras","detectar_personas","detectar_enemigos","detectar_armas","enviar_coordenada","name","setup","setTimeout","router","changeTabActive","num","document","getElementById","classList","remove","add","getLastImageFromFirebase","imgType","imgUrl","call_getLastImg","imgTypetxt","idComponent","curretnUrl","URL","window","location","href","ulrtxt","toString","urlSplit","split","urlLength","length","robotid","idRobot","type","imageWeb","urlimgage","console","log","src","connectRobot","disconnectRobot","call_detectar_caras_service","call_detectar_personas_service","call_detectar_enemigos_service","call_detectar_armas_robot","call_movement_service_robot","valor","call_enviar_cords","x","value","y"],"sourceRoot":"","sources":["/opt/lampp/htdocs/marselotech-web/marselotech-web/src/views/DashboardMarselotech.vue"],"sourcesContent":["<template>\n    <div class=\"grey\">\n        <!--Main Navigation-->\n        <header>\n\n            <!-- Sidebar -->\n            <div class=\"sidebar-fixed position-fixed sidebar\">\n\n                <router-link to=\"/\">\n                    <a  class=\" waves-effect mt-2\">\n                        <img src=\"http://localhost:8080/images/mtech-logo.png\" class=\"img-fluid\" alt=\"\">\n                    </a>\n                </router-link>\n                \n\n                <div class=\"list-group list-group-flush mt-2 \">\n                    \n                    <a href=\"#\" class=\"list-group-item active waves-effect sidebar txt1\">\n                        <i class=\"fas fa-chart-pie mr-3\"></i>Dashboard\n                    </a>\n                    <a href=\"#\" class=\"list-group-item list-group-item-action waves-effect sidebar txt1\">\n                        <i class=\"fas fa-user mr-3\"></i>Profile</a>\n                    <router-link to=\"/\">\n                        <a  class=\"list-group-item list-group-item-action waves-effect sidebar txt1\" >\n                        <i class=\"fas fa-backward mr-3\"></i>Volver</a>\n                    </router-link>\n                    \n                </div>\n\n            </div>\n            <!-- Sidebar -->\n\n        </header>\n        <!--Main Navigation-->\n\n        <!--Main layout-->\n        <main class=\"pt-1 mx-lg-5 \">\n            <div class=\"container-fluid mt-5\">\n                <!-- Heading -->\n                <div class=\"card mb-4 wow fadeIn\">\n\n                    <!--Card content-->\n                    <div class=\"card-body d-sm-flex justify-content-between\">\n\n                        <h4 class=\"mb-2 mb-sm-0 pt-1\">\n                            <router-link to=\"/\">\n                                <a>Marselotech</a>\n                            </router-link>\n                            <span class=\"txt1\">/</span>\n                            <span class=\"txt1\">Panel de control</span>\n                        </h4>\n                    </div>\n\n                </div>\n                <!-- Heading -->\n\n                <!--Grid row-->\n                <div class=\"row wow fadeIn\">\n\n                    <!--Grid column-->\n                    <div class=\"col-md-4 mb-4\">\n\n                        <!--Card-->\n                        <div class=\"card mb-4 dark_cards\">\n\n                            <!-- Card header -->\n                            <div id=\"robot_conectado\" class=\" text-danger card-header text-center d-flex justify-content-center\">\n                                Estado robot: desconectado\n                            </div>\n\n                            <div class=\"card-body text-center\">\n\n                                <div class=\"pl-5 pr-5 pb-2\">\n                                    <img src=\"http://localhost:8080/images/turtlebot3-burger.png\" class=\"img-fluid\" />\n                                </div>\n\n                                <div class=\"col-12\">\n                                    <button @click=\"connectRobot\" id=\"btn_conectar\" type=\"button\" class=\"btn btn-success btn-sm\">Conectar</button>\n                                    <button @click=\"disconnectRobot\" id=\"btn_desconectar\" type=\"button\" class=\"btn btn-danger btn-sm\">Desconectar</button>\n                                </div>\n\n                                <div class=\"col-12\">\n                                <button @click=\"call_movement_service_robot('delante')\" id=\"btn_adelante\" type=\"button\" class=\"btn btn-primary btn-sm \"><i class=\"fas fa-arrow-up fa-lg \" style=\"margin-left: 0;margin-right: 0;\"></i></button>\n                                <br>\n                                <div class=\"btn-group\" role=\"group\" aria-label=\"Basic example\">\n                                    <button @click=\"call_movement_service_robot('izquierda')\" id=\"btn_izquierda\" type=\"button\" class=\"btn btn-primary btn-sm text-center\"> <i class=\"fas fa-arrow-left fa-lg\" style=\"margin-left: 0;margin-right: 0;\"></i></button>\n                                    <button @click=\"call_movement_service_robot('parar')\" id=\"btn_parar\" type=\"button\" class=\"btn btn-warning btn-sm text-center\"> <i class=\"fas fa-stop fa-lg\" style=\"margin-left: 0;margin-right: 0;\"></i></button>\n                                    <button @click=\"call_movement_service_robot('derecha')\" id=\"btn_derecha\" type=\"button\" class=\"btn btn-primary btn-sm text-center\"> <i class=\"fas fa-arrow-right fa-lg\" style=\"margin-left: 0;margin-right: 0;\"></i></button>\n                                </div>\n                                <br>\n                                <button @click=\"call_movement_service_robot('atras')\" id=\"btn_atras\" type=\"button\" class=\"btn btn-primary btn-sm\"><i class=\"fas fa-arrow-down fa-lg\" style=\"margin-left: 0;margin-right: 0;\"></i></button>\n                                </div>\n                            </div>\n\n                        </div>\n                        <!--/.Card-->\n\n                    </div>\n                    <!--Grid column-->\n\n                    <!--Grid column-->\n                    <div class=\"col-md-8 mb-4\">\n\n                        <!--Card-->\n                        <div class=\"card\">\n\n                            <!-- Card header -->\n                            <div class=\"card-header txt1\">\n                                <!-- Tabs navs -->\n                                <ul class=\"nav nav-tabs mb-3\" id=\"ex-with-icons\" role=\"tablist\">\n                                    <li class=\"nav-item\" role=\"presentation\">\n                                        <a @click=\"changeTabActive('1')\"  class=\"nav-link active\" id=\"ex-with-icons-tab-1\" data-toggle=\"tab\" data-mdb-toggle=\"tab\" href=\"#a\" role=\"tab\"\n                                        aria-controls=\"ex-with-icons-tabs-1\" aria-selected=\"true\"><i class=\"fas fa-low-vision mr-2\"></i>Vigilancia</a>\n                                    </li>\n                                    <li @click=\"changeTabActive('2')\"  class=\"nav-item\" role=\"presentation\">\n                                        <a class=\"nav-link\" id=\"ex-with-icons-tab-2\" data-toggle=\"tab\" data-mdb-toggle=\"tab\" href=\"#b\" role=\"tab\"\n                                        aria-controls=\"ex-with-icons-tabs-2\" aria-selected=\"false\"><i class=\"fas fa-map-marker-alt mr-2\"></i>Exploración</a>\n                                    </li>\n                                    <li @click=\"changeTabActive('3')\"  class=\"nav-item\" role=\"presentation\">\n                                        <a class=\"nav-link\" id=\"ex-with-icons-tab-3\" data-toggle=\"tab\" data-mdb-toggle=\"tab\" href=\"#c\" role=\"tab\"\n                                        aria-controls=\"ex-with-icons-tabs-3\" aria-selected=\"false\"><i class=\"fas fa-luggage-cart mr-2\"></i>Soporte</a>\n                                    </li>\n                                </ul>\n                                <!-- Tabs navs -->\n                            </div>\n\n                            <!--Card content-->\n                            <div class=\"card-body\">\n                                <div class=\"tab-content\">\n                                    <div class=\"tab-pane active\" id=\"a\">\n\n                                        <div>\n                                            <button @click=\"call_detectar_caras_service\" type=\"button\" class=\"btn btn-primary btn-sm\">Reconocimiento facial</button>\n                                            <button @click=\"call_detectar_personas_service\" type=\"button\" class=\"btn btn-primary btn-sm\">Detectar persona</button>\n                                            <button @click=\"call_detectar_enemigos_service\" type=\"button\" class=\"btn btn-primary btn-sm\">Detectar enemigo</button>\n                                        </div>\n\n                                        <div class=\"text-center\">\n                                            <a class=\"ripple img-fluid\">\n                                                <img id=\"tabApoyo\"\n                                                        alt=\"example\"\n                                                        class=\"img-fluid rounded\"\n                                                        src=\"https://mdbcdn.b-cdn.net/img/new/standard/city/044.webp\"\n                                                />\n                                            </a>\n                                        </div>\n\n                                    </div>\n                                    <div class=\"tab-pane\" id=\"b\">\n                                        <div class=\"row\">\n                                            <div class=\"col\">\n                                                <div class=\"form-outline input-group-sm\">\n                                                    <label class=\"form-label txt1\" >Coordenadas del robot</label>\n                                                    <div class=\"d-flex\">\n                                                        <p class=\"txt1\">X:</p>\n                                                        <p class=\"txt1 pl-2\" id=\"pos_x\">Sin conexión</p>\n                                                    </div>\n                                                    <div class=\"d-flex\">\n                                                        <p class=\"txt1\">Y:</p>\n                                                        <p class=\"txt1 pl-2\" id=\"pos_y\">Sin conexión</p>\n\n                                                    </div>\n                                                </div>\n                                            </div>\n                                            <div class=\"col\">\n                                                <div class=\"form-outline input-group-sm\">\n                                                    <label class=\"form-label txt1\" >Nuevas coordenadas</label>\n                                                    <input type=\"number\" id=\"x_input\"  placeholder=\"x\" class=\"form-control\"/>\n                                                    <input type=\"number\" id=\"y_input\"  placeholder=\"y\" class=\"form-control mt-3\" />\n                                                    <div class=\"text-right\">\n                                                        <button :click=\"call_enviar_cords\" id=\"btn_enviar_coordenada\"  type=\"button\" class=\"btn btn-primary btn-sm\">Enviar</button>\n                                                    </div>\n                                                </div>\n                                            </div>\n                                        </div>\n                                    </div>\n                                    <div class=\"tab-pane\" id=\"c\">\n                                        <button :click=\"call_detectar_armas_robot\" type=\"button\" class=\"btn btn-primary btn-sm\">Detectar armas</button>\n\n                                        <div class=\"text-center\">\n                                            <a class=\"ripple img-fluid\">\n                                                <img   id=\"tabGuns\"\n                                                        alt=\"example\"\n                                                        class=\"img-fluid rounded\"\n                                                        src=\"https://mdbcdn.b-cdn.net/img/new/standard/city/044.webp\"\n                                                />\n                                            </a>\n                                        </div>\n\n                                    </div>\n                                </div>\n\n                            </div>\n\n                        </div>\n                        <!--/.Card-->\n\n                    </div>\n                    <!--Grid column-->\n\n\n\n                </div>\n                <!--Grid row-->\n\n            </div>\n        </main>\n        <!--Main layout-->\n    </div>\n</template>\n\n<script>\nimport { reactive, onMounted } from 'vue';\nimport { useRouter } from 'vue-router';\nimport {getLastImage} from '@/firebase'\nimport { connect, disconnect, call_delante_service, detectar_caras, detectar_personas, detectar_enemigos, detectar_armas, enviar_coordenada} from '@/robot';\n\n\nexport default{\n    name: 'DashboardMarselotech',\n    setup(){\n\n        onMounted(()=>{\n            setTimeout(() => {\n\n                //w\n               /* document.addEventListener(\"keydown\", (w) => {\n                    call_movement_service_robot('delante');\n                })\n\n                document.addEventListener('input', (w) => {\n                    logMessage(`Key \"${w.data}\" input  [event: input]`);\n                    call_movement_service_robot('delante');\n                });\n\n\n                //a\n                document.addEventListener(\"keydown\", (a) => {\n                    call_movement_service_robot('izquierda');\n                })\n\n                document.addEventListener('input', (a) => {\n                    logMessage(`Key \"${a.data}\" input  [event: input]`);\n                    call_movement_service_robot('izquierda');\n                });\n\n\n                //s\n                document.addEventListener(\"keydown\", (s) => {\n                    call_movement_service_robot('atras')\n                })\n\n                document.addEventListener('input', (s) => {\n                    logMessage(`Key \"${s.data}\" input  [event: input]`);\n                    call_movement_service_robot('atras')\n                });\n\n\n                //d\n                document.addEventListener(\"keydown\", (d) => {\n                    call_movement_service_robot('derecha')\n                })\n\n                document.addEventListener('input', (d) => {\n                    logMessage(`Key \"${d.data}\" input  [event: input]`);\n                    call_movement_service_robot('derecha')\n                });*/\n                \n\n            },1500);\n        });\n        \n        const router = useRouter();\n\n        const changeTabActive = (num) =>{\n            document.getElementById(\"ex-with-icons-tab-1\").classList.remove('active');\n            if(num === 1){\n                //ex-with-icons-tab-1\n                document.getElementById(\"ex-with-icons-tab-1\").classList.add('active');\n                document.getElementById(\"ex-with-icons-tab-2\").classList.remove('active');\n                document.getElementById(\"ex-with-icons-tab-3\").classList.remove('active');\n            }else if(num === 2){\n                // ex-with-icons-tab-2\n                //ex-with-icons-tab-1\n                document.getElementById(\"ex-with-icons-tab-2\").classList.add('active');\n                document.getElementById(\"ex-with-icons-tab-1\").classList.remove('active');\n                document.getElementById(\"ex-with-icons-tab-3\").classList.remove('active');\n            }else if(num === 3){\n                // ex-with-icons-tab-3\n                document.getElementById(\"ex-with-icons-tab-3\").classList.add('active');\n                document.getElementById(\"ex-with-icons-tab-2\").classList.remove('active');\n                document.getElementById(\"ex-with-icons-tab-1\").classList.remove('active');\n            }\n        }\n\n        const getLastImageFromFirebase = async (imgType) => {\n            /*caras, persona, color, arma*/\n           const imgUrl = await getLastImage(imgType);\n           return imgUrl;\n        }\n\n        const call_getLastImg = async (imgTypetxt,idComponent) => {\n\n            var curretnUrl = new URL(window.location.href);\n            let ulrtxt = curretnUrl.toString();\n            let urlSplit = ulrtxt.split(\"/\");\n            let urlLength = urlSplit.length;\n            let robotid = urlSplit[urlLength-1];\n            let imgType = {idRobot: robotid, type: imgTypetxt}\n            let imageWeb = document.getElementById(idComponent);\n            const urlimgage = await getLastImageFromFirebase(imgType);\n            console.log(urlimgage);\n            imageWeb.src = urlimgage;\n\n        }\n\n\n        const  connectRobot = connect;\n\n        const disconnectRobot = disconnect;\n\n        const call_detectar_caras_service  = () =>{\n            call_getLastImg(\"caras\",\"tabApoyo\");\n            detectar_caras();\n        };\n\n        const call_detectar_personas_service = () => {\n            call_getLastImg(\"persona\",\"tabApoyo\")\n            detectar_personas();\n        };\n\n        const call_detectar_enemigos_service = () =>{\n            call_getLastImg(\"color\",\"tabApoyo\");\n            detectar_enemigos();\n        };\n\n        const call_detectar_armas_robot = () => {\n            call_getLastImg(\"arma\",\"tabApoyo\");\n            detectar_armas();\n        };\n\n        const call_movement_service_robot = async (valor) => {\n           await call_delante_service(valor);\n        };\n\n        const call_enviar_cords = async () => {\n            let x = document.getElementById(\"x_input\").value;\n            let y = document.getElementById(\"y_input\").value;\n            await enviar_coordenada(x,y);\n\n        };\n\n        \n        \n        return { call_getLastImg, changeTabActive, connectRobot, disconnectRobot, call_movement_service_robot, call_detectar_caras_service, call_detectar_personas_service, call_detectar_enemigos_service, call_detectar_armas_robot, call_enviar_cords};\n    }\n};\n\n</script>\n\n    <style>\n\n        .map-container{\n            overflow:hidden;\n            padding-bottom:56.25%;\n            position:relative;\n            height:0;\n        }\n        .map-container iframe{\n            left:0;\n            top:0;\n            height:100%;\n            width:100%;\n            position:absolute;\n        }\n\n        :root{\n            /* dark color */\n            --dark-h: 0;\n            --dark-s: 0%;\n            --dark-l: 12%;\n            --dark-color: var(--dark-h), var(--dark-s), var(--dark-l);\n            /* light color */\n            --light-h: 0;\n            --light-s: 0%;\n            --light-l: 100%;\n            --light-color: var(--light-h), var(--light-s), var(--light-l);\n\n            --bg-color-one: var(--dark-color);\n            --bg-color-two: var(--dark-h), var(--dark-s), calc(var(--dark-l) + 7%);\n            --t-color-one: var(--light-color);\n\n        }\n\n        .card {\n            background-color: hsl(var(--bg-color-two)) !important;\n        }\n\n        .sidebar{\n            background-color: hsl(var(--bg-color-two)) !important;\n        }\n\n        .grey{\n            background-color: hsl(var(--bg-color-one)) !important;\n        }\n\n        .txt1{\n            color: hsl(var(--t-color-one)) !important;\n        }\n    </style>"]},"metadata":{},"sourceType":"module"}